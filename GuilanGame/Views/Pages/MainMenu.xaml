<Page
    x:Class="GuilanGame.Views.Pages.MainMenu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ffme="clr-namespace:Unosquare.FFME;assembly=ffme.win"
    xmlns:local="clr-namespace:GuilanGame.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainMenu"
    d:DesignHeight="768"
    d:DesignWidth="1024"
    FontFamily="{DynamicResource Fonts.IRANSans}"
    Loaded="Page_Loaded"
    mc:Ignorable="d">
    <Page.Background>
        <SolidColorBrush Color="Black" />
    </Page.Background>
    <Grid>

        <Frame
            x:Name="PageFrame"
            JournalOwnership="UsesParentJournal"
            Navigating="PageFrame_Navigating"
            NavigationUIVisibility="Hidden"
            Source="pack://application:,,,/GuilanGame;component/Views/Pages/Main/MasterPage.xaml" />
    </Grid>

</Page>